export class UserItem {

    name: string;
    email: string;

    consructor(name: string, email: string) {
	this.name = name;
	this.email = email;
    }

    lookup(query: string): string {
	if (query == this.name) {
	    return this.email;
	} else {
	    return this.name;
	}
    }

    name(): string {
	return this.name;
    }

    email(): string {
	return this.email;
    }

}
